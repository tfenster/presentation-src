{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westeurope"
        },
        "networkInterfaceName": {
            "value": "cosmo-big380"
        },
        "enableAcceleratedNetworking": {
            "value": true
        },
        "networkSecurityGroupName": {
            "value": "cosmo-big-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "cosmo-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.1.0.0/24"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.1.0.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "cosmo-big-ip"
        },
        "publicIpAddressType": {
            "value": "Dynamic"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "virtualMachineName": {
            "value": "cosmo-big"
        },
        "virtualMachineRG": {
            "value": "cosmo-bug"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "ephemeralDiskType": {
            "value": "Local"
        },
        "virtualMachineSize": {
            "value": "Standard_D16s_v3"
        },
        "adminUsername": {
            "value": "CosmoAdmin"
        },
        "adminPassword": {
            "value": null
        }
    }
}